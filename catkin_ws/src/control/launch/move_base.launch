<?xml version="1.0" ?>
<!-- Navigation stack launch file. -->

<!-- TODO: fill out all fields and specify for our robot. -->

<launch>
  <master auto="start"/>

  <node name="move_base"
        pkg="move_base" type="move_base"
        respawn="true" output="screen">

    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />

    <rosparam file="$(find control)/config/move_base_config.yaml" command="load"/>
    <rosparam
        file="$(find control)/config/costmap_common_params.yaml"
        command="load" ns="global_costmap" />
    <rosparam
        file="$(find control)/config/costmap_common_params.yaml"
        command="load" ns="local_costmap" />
    <rosparam
        file="$(find control)/config/local_costmap_params.yaml"
        command="load" />
    <rosparam
        file="$(find control)/config/global_costmap_params.yaml"
        command="load" />
    <rosparam
        file="$(find control)/config/base_local_planner_params.yaml"
        command="load" />
  </node>

</launch>
